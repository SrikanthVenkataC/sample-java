pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'javac Hello.java'
            }
        }
        stage('Run') {
            steps {
                echo 'Run the Hello java prog..'
                sh 'java Hello'
                
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
               sh 'aws s3 ls'
               aws s3 ls  
            }
        }
    }
}
